{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"exclude": [
            "node_modules",
			"docs",
			"logs",
			"coverage",
			"lint",
			"test",
            "plugins"
        ]
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"dateFormat": "MMMM D, YYYY",
		"outputSourceFiles": true,
		"outputSourcePath": true,
		"systemName": "Moxai (mɒks-eɪ) - Express Middleware for API mocks using Open API Initiative (OAI)",
		"footer": "",
		"copyright": "",
		"navType": "vertical",
		"theme": "flatly",
		"linenums": true,
		"collapseSymbols": false,
		"inverseNav": true,
		"protocol": "html://",
		"methodHeadingReturns": false
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	}
}
